@model GastroFaza.Models.DTO.ReservationDto

@{
    ViewData["Title"] = "Update";
}

<h1>Update</h1>

<h4>UpdateReservationDto</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post" action="@Url.Action("Edit", "Reservation")" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TableId" class="control-label"></label>
                <input asp-for="TableId" class="form-control" />
                <span asp-validation-for="TableId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DataOfReservation" class="control-label"></label>
                <input asp-for="DataOfReservation" class="form-control" />
                <span asp-validation-for="DataOfReservation" class="text-danger"></span>
            </div>
            <br /> <br />
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>